[
  {
    "name": "clair",
    "image": "434313288222.dkr.ecr.us-east-1.amazonaws.com/prod/ellucian-clair:0.0.5",
    "cpu": 1024,
    "memory": 1024,
    "essential": true,
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group}",
            "awslogs-region": "${region}",
            "awslogs-stream-prefix": "clair"
        }
    },
    "environment": [
        {
            "name": "CONFIG_PARAMETER_REGION",
            "value": "${region}"
        },
        {
            "name": "CONFIG_PARAMETER_NAME",
            "value": "${config_parameter_name}"
        }
    ],
    "portMappings": [
      {
        "containerPort": 6060,
        "hostPort": 6060
      }
    ]
  }
]